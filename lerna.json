{
  "packages": [
    "packages/*"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "version": {
      "conventionalCommits": true
    },
    "publish": {
      "message": "chore(release): publish %s",
      "verifyAccess": false,
      "verifyRegistry": false,
      "registry": "https://registry.npmjs.org/"
    }
  },
  "version": "independent"
}

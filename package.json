{
  "version": "3.1.2",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "lint": "eslint .",
    "version": "bumpp --no-commit --no-tag --no-push",
    "release": "pnpm -r publish --no-git-checks",
    "up": "taze major -I",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@vividcat/eslint-config": "workspace:*",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "taze": "^0.11.2"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "eslintConfig": {
    "extends": "@vividcat",
    "ignorePatterns": [
      "fixtrue"
    ]
  }
}
